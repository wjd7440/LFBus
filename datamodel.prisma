type User {
  id: ID! @id
  name: String!
}

type BusStation {
  BUS_NODE_ID: ID! @id
  BUS_STOP_ID: Int!
  BUSSTOP_NM: String!
  BUSSTOP_ENG_NM: String!
  GPS_LATI: Float!
  GPS_LONG: Float!
  BUSSTOP_SEQ: String
  BUSSTOP_TP: String
  ROAD_NM: String
  ROAD_NM_ADDR: String
  ROUTE_CD: Int
  TOTAL_DIST: Int
}

type BusInfo{
  BUS_TYPE: Int!
  CAR_REG_NO: ID! @id
  CHARACTER: Int!
  COMP_CD: Int!
  ROUTE_CD: Int!
}